{
  "servers": {
    "context-ai-mcp-server": {
      "type": "stdio",
      "command": "node",
      "args": ["dist/index.js"],
      "env": {
        "WORKSPACE_PATH": "${workspaceFolder}"
      }
    },
    "github-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@github/github-mcp-server"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${env:GITHUB_TOKEN}"
      }
    }
  }
}
